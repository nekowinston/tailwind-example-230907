{
  "devDependencies": {
    "@catppuccin/tailwindcss": "^0.1.6",
    "concurrently": "^8.2.1",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.3"
  },
  "scripts": {
    "build": "tailwindcss -o ./src/style.css",
    "dev": "concurrently --kill-others 'tailwindcss -o ./src/style.css -w' 'serve ./src'",
    "start": "serve ./src"
  }
}
